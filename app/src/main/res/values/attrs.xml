<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="setting_view">
        <attr name="item_titleText" format="string"/>
        <attr name="item_detailText" format="string"/>
        <attr name="item_showDivider" format="boolean"/>
        <attr name="item_icon" format="reference|color"/>
    </declare-styleable>


    <declare-styleable name="input_text">
        <attr name="item_showIcon" format="boolean"/>
        <attr name="item_close_icon" format="reference|color"/>
        <attr name="item_hint_text" format="string"/>
        <attr name="item_InputType">
            <enum name="text" value="0"/>
            <enum name="phone" value="1"/>
            <enum name="password" value="2"/>
        </attr>
    </declare-styleable>
</resources>